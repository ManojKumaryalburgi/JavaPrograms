package arrays;

public class ToFindMissingNumberInArray {

	public static void main(String[] args) {
		int a []= {1,2,3,5};
		int sum1=0;
		for (int i = 0; i < a.length; i++) 
		{
			sum1=sum1+a[i];
			
		}
		
		System.out.println("sum of  array is"+sum1);
		
		int sum2=0;
		for(int i=0;i<=5;i++)
		{
			sum2=sum2+a[i];
		}
		
		System.out.println("sum of missing element including array number:"+sum2);
		
		

	}

}
