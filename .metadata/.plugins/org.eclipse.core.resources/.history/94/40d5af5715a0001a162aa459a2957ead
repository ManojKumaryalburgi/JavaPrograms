package strings;

public class Converting_CharacterArrayToString {

	public static void main(String[] args) {
		
		/*we can convert char array to sting in three ways
		 * 1)using string(char c [])
		 * 2)using valueof(char c []) 
		 * 3)with out using built in
		 */
		
		//1
		char c []= {'m','a','n','o','j'};
		 
		String s = new String(c);
		System.out.println(s);
		
		

	}

}
