package arrays;

public class ToFindSecondMininArray {

	public static void main(String[] args) {
		
		int c []= {23,12,34,50,98};
		
		for (int i = 0; i < c.length; i++)
		{
			{
				int temp=0;
				if(c[j]<c[j+1])
				{
					temp=c[j];
					c[j]=c[j+1];
					c[j+1]=temp;
				}
				
			}
			
		}
		
		for (int i = 0; i < c.length; i++) 
		{
			System.out.println("second minimum element in Array is :"+c[i]);
			
		}

	}

}
